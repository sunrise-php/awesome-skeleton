{
    "type": "project",
    "name": "sunrise/awesome-skeleton",
    "description": "Awesome Skeleton for modern development on PHP 7.1+",
    "keywords": ["fenric", "sunrise", "skeleton", "psr-2", "psr-3", "psr-7", "psr-11", "psr-15", "psr-17"],
    "homepage": "https://github.com/sunrise-php/awesome-skeleton",
    "license": "MIT",
    "authors": [{
        "name": "Anatoly Fenric",
        "email": "anatoly@fenric.ru",
        "homepage": "https://anatoly.fenric.ru/"
    }],
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "middlewares/payload": "^2.1",
        "middlewares/response-time": "^1.1",
        "middlewares/whoops": "^1.2",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "sunrise/http-factory": "^1.0",
        "sunrise/http-router": "^1.0",
        "sunrise/http-router-annotations-support": "^1.0",
        "symfony/validator": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always --coverage-text",
            "phpcs"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public/"
    }
}
