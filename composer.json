{
    "type": "project",
    "name": "sunrise/awesome-skeleton",
    "homepage": "https://github.com/sunrise-php/awesome-skeleton",
    "description": "Awesome Skeleton for modern development on PHP 7.1+",
    "license": "MIT",
    "keywords": [
        "fenric",
        "sunrise",
        "skeleton"
    ],
    "require": {
        "php": "^7.3",
        "php-di/php-di": "^6.0",
        "monolog/monolog": "^2.0",
        "sunrise/http-factory": "^1.0",
        "sunrise/http-router": "^2.1",
        "sunrise/http-router-openapi": "^1.1",
        "doctrine/orm": "^2.7",
        "doctrine/migrations": "^2.2",
        "arus/doctrine-repository-factory": "^1.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/validator": "^5.0",
        "spiral/roadrunner": "^1.5",
        "middlewares/payload": "^3.0",
        "justinrainbow/json-schema": "^5.2",
        "filp/whoops": "^2.7",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.20",
        "sunrise/coding-standard": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always --coverage-text",
            "phpcs --ignore=src/Migration"
        ],
        "serve": "php -S 0.0.0.0:3000 -t public/"
    }
}
